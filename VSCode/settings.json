{
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.colorTheme": "Atom One Light",
    "git.openRepositoryInParentFolders": "never",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 50,
    "editor.minimap.showSlider": "always",
    "editor.fontFamily": "'JetBrains Mono'",
    "editor.fontLigatures": true,
    "editor.fontWeight": "400",
    "editor.lineHeight": 23,
    "workbench.colorCustomizations": {
        "editor.background": "#F4F4F4",
        "editor.foreground": "#000000",
        "sideBarTitle.foreground": "#000000",
        "sideBarSectionHeader.border": "#7d7a7a",
        "sideBar.border": "#7d7a7a",
        "sideBar.background": "#fefefe",
        "sideBar.foreground": "#000000",
        "editorGroupHeader.border": "#7d7a7a",
        "editorGroup.border": "#7d7a7a",
        "tab.activeBorder": "#6ea14f",
        "tab.border": "#7d7a7a",
        "tab.hoverBorder": "#6ea14f",
        "tab.unfocusedHoverBorder": "#6ea14f",
        "tab.hoverForeground": "#000000",
        "tab.inactiveForeground": "#000000",
        "tab.activeForeground": "#000000",
        "tab.activeBackground": "#6ea14f08",
        "tab.inactiveBackground": "#ffffff",
        "titleBar.border": "#7d7a7a",
        "editor.selectionHighlightBackground": "#4a121223",
        "editor.selectionBackground": "#ae9c9c2c",
        "editor.wordHighlightBackground": "#ff820522",
        "editor.findMatchHighlightBackground": "#daa5204d",
        "editor.findRangeHighlightBackground": "#daa5204d",
        "editor.inactiveSelectionBackground": "#daa5204d",
        "editor.findMatchBackground": "#daa5204d",
        "breadcrumb.background": "#00AB75",
        "breadcrumb.foreground": "#FFFFFF",
        "breadcrumb.activeSelectionForeground": "#FFFFFF",
        "breadcrumb.focusForeground": "#FFFFFF",
        "breadcrumbPicker.background": "#ffffff",
    },
    "workbench.iconTheme": "material-icon-theme",
    "editor.tokenColorCustomizations": {
        "[Visual Studio Light]": {
            // https://marketplace.visualstudio.com/items?itemName=emilast.LogFileHighlighter
            "textMateRules": [
                {
                    "scope": "log.error",
                    "settings": {
                        // "foreground": "#00ffae",
                        // "fontStyle": "bold"
                    }
                },
                {
                    "scope": "log.warning",
                    "settings": {
                        // "foreground": "#f4ad42",
                        // "fontStyle": ""
                    }
                }
            ]
        }
    }
}
